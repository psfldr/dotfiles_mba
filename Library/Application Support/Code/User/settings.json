    "workbench.colorTheme": "Base16 Dark Ashes",
    "editor.fontFamily": "Cica, Menlo, Monaco, 'Courier New', monospace",
    "window.nativeFullScreen": false,
    "terminal.integrated.rendererType": "dom",
